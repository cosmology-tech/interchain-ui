{
  "name": "@interchain-ui/react",
  "version": "1.3.3",
  "description": "UI building blocks for Interchain builders",
  "author": "yyyyaaa <phatghaa@gmail.com>",
  "homepage": "https://github.com/cosmology-tech/ui-kit/tree/main/packages/react#readme",
  "license": "SEE LICENSE IN LICENSE",
  "source": "src/index.ts",
  "main": "dist/interchain-ui-kit-react.cjs.js",
  "module": "dist/interchain-ui-kit-react.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "default": "./dist/index.d.ts"
      },
      "module": "./dist/interchain-ui-kit-react.esm.js",
      "import": "./dist/interchain-ui-kit-react.esm.js",
      "default": "./dist/interchain-ui-kit-react.cjs.js"
    },
    "./styles": "./dist/interchain-ui-kit-react.cjs.css",
    "./package.json": "./package.json"
  },
  "targets": {
    "main": {
      "optimize": true,
      "includeNodeModules": [
        "@formkit/auto-animate"
      ]
    },
    "module": {
      "optimize": true,
      "includeNodeModules": [
        "@formkit/auto-animate"
      ]
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "@fastify/deepmerge": "^1.3.0",
    "@floating-ui/react": "^0.25.0",
    "@formkit/auto-animate": "1.0.0-beta.6",
    "@vanilla-extract/css": "^1.12.0",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.4.0",
    "@zag-js/dialog": "^0.9.2",
    "@zag-js/number-input": "^0.9.2",
    "@zag-js/react": "^0.9.2",
    "animejs": "^3.2.1",
    "bignumber.js": "^9.1.1",
    "clsx": "^1.2.1",
    "copy-to-clipboard": "^3.3.3",
    "immer": "^9.0.19",
    "lodash": "^4.17.21",
    "motion": "^10.16.2",
    "rainbow-sprinkles": "^0.17.0",
    "zustand": "^4.3.8"
  },
  "peerDependencies": {
    "react": "^18.x",
    "react-dom": "^18.x"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "build:dev": "NODE_ENV=development parcel build",
    "build:analyze": "parcel build --reporter @parcel/reporter-bundle-analyzer",
    "clean": "rimraf ./dist",
    "check": "tsc --noEmit",
    "ci": "yarn build && yarn check",
    "minifyCss": "cleancss --batch --batch-suffix '' dist/**/*.css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@parcel/compressor-brotli": "2.8.3",
    "@parcel/compressor-gzip": "2.8.3",
    "@parcel/optimizer-swc": "2.8.3",
    "@parcel/reporter-bundle-analyzer": "2.8.3",
    "@parcel/transformer-typescript-tsc": "2.8.3",
    "@parcel/transformer-typescript-types": "2.8.3",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/blocks": "^7.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/react-vite": "^7.1.0",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@vanilla-extract/parcel-transformer": "^1.0.0",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "parcel": "2.8.3",
    "parcel-optimizer-unlink-css": "^1.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.1.0",
    "vite-plugin-replace": "^0.1.1"
  },
  "gitHead": "bdcf939cbdcf307d7c1c97d7b26ef293c2beaf01"
}
