{
  "name": "@interchain-ui/vue",
  "version": "1.2.55",
  "type": "module",
  "description": "UI building blocks for Interchain builders",
  "author": "yyyyaaa <phatghaa@gmail.com>",
  "homepage": "https://github.com/cosmology-tech/interchain-ui/tree/main/packages/vue#readme",
  "license": "SEE LICENSE IN LICENSE",
  "source": "src/index.ts",
  "main": "/dist/interchain-ui-vue.cjs.cjs",
  "module": "/dist/interchain-ui-vue.esm.js",
  "types": "/dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/interchain-ui-vue.esm.js",
      "import": "./dist/interchain-ui-vue.cjs.js",
      "require": "./dist/interchain-ui-vue.cjs.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./globalStyles": {
      "import": "./dist/styles/global.css.css",
      "require": "./dist/styles/global.css.css"
    },
    "./package.json": "./package.json"
  },
  "targets": {
    "main": {
      "optimize": true,
      "includeNodeModules": [
        "client-only",
        "@formkit/auto-animate"
      ]
    },
    "module": {
      "optimize": true,
      "includeNodeModules": [
        "client-only",
        "@formkit/auto-animate"
      ]
    },
    "globalStyles": {
      "source": "src/styles/global.css.ts",
      "context": "browser",
      "distDir": "./dist"
    }
  },
  "sideEffects": false,
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:preview": "vite preview",
    "clean": "rimraf ./dist",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "sb": "storybook dev -p 6007",
    "sb:build": "storybook build"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.10",
    "@formkit/auto-animate": "^0.8.2",
    "@vanilla-extract/css": "^1.15.5",
    "@vanilla-extract/dynamic": "^2.1.2",
    "@vanilla-extract/recipes": "^0.5.5",
    "animejs": "^3.2.2",
    "bignumber.js": "^9.1.2",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "rainbow-sprinkles": "^0.17.2",
    "vue-final-modal": "^4.5.4",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.8.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/vue3": "^8.2.9",
    "@storybook/vue3-vite": "^8.2.9",
    "@types/animejs": "^3.1.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.16.2",
    "@vanilla-extract/vite-plugin": "^4.0.15",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue.ts/complex-types": "^0.6.0",
    "@vue/compiler-sfc": "^3.4.38",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.27.0",
    "flexsearch": "^0.7.43",
    "globby": "^14.0.2",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "shiki-es": "^0.14.0",
    "storybook": "^8.2.9",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "vscode-oniguruma": "^2.0.1",
    "vscode-textmate": "^9.1.0",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.24"
  },
