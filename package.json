{
  "name": "interchain-ui",
  "description": "UI building blocks for Interchain builders",
  "version": "0.1.0",
  "publishConfig": {
    "access": "restricted"
  },
  "private": true,
  "scripts": {
    "bootstrap": "yarn install; lerna bootstrap;",
    "compile": "node ./compiler",
    "compile:prod": "cross-env NODE_ENV=production node ./compiler",
    "c:react": "node ./compiler -p react",
    "b:react": "lerna run --stream --scope=@interchain-ui/react build",
    "c:vue": "node ./compiler -p vue",
    "b:vue": "lerna run --stream --scope=@interchain-ui/vue build",
    "dev": "node ./compiler/dev",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006 --no-manager-cache",
    "story:react": "lerna run --scope @interchain-ui/react-example --stream dev",
    "clean": "rimraf packages/{react,vue}/{src,dist,lib,types,node_modules,stats.html}; rimraf ./node_modules",
    "clean:assets": "rimraf packages/{react,vue}/{src,dist,lib,types,stats.html}",
    "lerna": "lerna run",
    "pub:beta": "lerna publish --no-private --preid beta",
    "lint": "run-p lint:**",
    "lint:scripts": "eslint --fix \"./src/**/*.tsx\"",
    "build": "lerna run compile"
  },
  "devDependencies": {
    "@angular/core": "^14.3.0",
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@builder.io/eslint-plugin-mitosis": "^0.0.14",
    "@builder.io/mitosis-cli": "0.0.77",
    "@builder.io/qwik": "^0.100.0",
    "@custom-elements-manifest/analyzer": "^0.8.0",
    "@fastify/deepmerge": "^1.3.0",
    "@formkit/auto-animate": "1.0.0-beta.6",
    "@parcel/packager-ts": "2.8.3",
    "@parcel/watcher": "^2.2.0",
    "@swc/helpers": "^0.5.0",
    "@types/animejs": "^3.1.7",
    "@types/node": "18.15.11",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/babel-preset-app": "^5.0.8",
    "@zag-js/dialog": "^0.15.0",
    "@zag-js/number-input": "^0.15.0",
    "@zag-js/react": "^0.15.0",
    "@zag-js/vue": "^0.15.0",
    "autoprefixer": "^10.4.14",
    "command-line-args": "^5.2.1",
    "copy-to-clipboard": "^3.3.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "execa": "^5.1.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.5",
    "gluegun": "^5.1.2",
    "husky": "^8.0.3",
    "lerna": "^6.6.1",
    "listr2": "^5.0.8",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "ora": "^5.4.1",
    "prettier": "^2.8.8",
    "rainbow-sprinkles": "^0.17.0",
    "replace": "^1.2.2",
    "replace-in-file": "^6.3.5",
    "rimraf": "^5.0.0",
    "type-fest": "^4.2.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.4.4"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.12.0",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/recipes": "^0.4.0",
    "animejs": "^3.2.1",
    "clsx": "^1.2.1",
    "immer": "^9.0.21",
    "motion": "^10.16.2",
    "vue": "^3.2.47",
    "zustand": "^4.3.7"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}
