{
  "name": "interchain-ui",
  "description": "UI building blocks for Interchain builders",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmology-tech/interchain-ui.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "private": true,
  "scripts": {
    "bootstrap": "yarn install; lerna bootstrap;",
    "compile": "node ./compiler",
    "compile:prod": "cross-env NODE_ENV=production node ./compiler",
    "mitosis:react": "rm -rf packages/react/src && node ./compiler/frameworks/react.compile.js",
    "mitosis:vue": "rm -rf packages/vue/src && node ./compiler/frameworks/vue.compile.js",
    "c:react": "node ./compiler -p react",
    "b:react": "lerna run --stream --scope=@interchain-ui/react build",
    "c:vue": "node ./compiler -p vue",
    "b:vue": "lerna run --stream --scope=@interchain-ui/vue build",
    "dev": "node ./compiler/dev",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006 --no-manager-cache",
    "story:react": "lerna run --scope @interchain-ui/react-example --stream dev",
    "lerna": "lerna run",
    "pub:beta": "lerna publish --no-private --preid beta",
    "clear": "npm run clear:pcache & npm run clear:builds && npm run clear:artifacts & npm run clear:node_modules",
    "clear:artifacts": "find ./packages -name src -type d | grep -vE '.*-example.*' | xargs rm -rf",
    "clear:builds": "find ./packages -name dist -type d | grep -vE '.*-example.*' | xargs rm -rf",
    "clear:pcache": "rm -rf ./.parcel-cache",
    "clear:node_modules": "rm -rf ./node_modules && rm -rf ./packages/*/node_modules",
    "lint": "run-p lint:**",
    "lint:scripts": "eslint --fix \"./src/**/*.tsx\"",
    "build": "lerna run compile"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.22.14",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.11",
    "@builder.io/eslint-plugin-mitosis": "^0.0.14",
    "@builder.io/mitosis-cli": "0.0.96",
    "@builder.io/qwik": "^0.100.0",
    "@formkit/auto-animate": "^0.8.1",
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/watcher": "^2.4.1",
    "@react-aria/i18n": "^3.8.4",
    "@react-aria/numberfield": "^3.9.1",
    "@react-aria/utils": "^3.21.1",
    "@react-stately/numberfield": "^3.6.2",
    "@swc/helpers": "^0.5.0",
    "@types/animejs": "^3.1.7",
    "@types/node": "^20.5.8",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vanilla-extract/dynamic": "^2.1.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/babel-preset-app": "^5.0.8",
    "autoprefixer": "^10.4.14",
    "command-line-args": "^5.2.1",
    "copy-to-clipboard": "^3.3.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "execa": "^5.1.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.5",
    "gluegun": "^5.1.2",
    "husky": "^8.0.3",
    "lerna": "^6.6.1",
    "lerna-changelog": "^2.2.0",
    "listr2": "^5.0.8",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "ora": "^5.4.1",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.8",
    "rainbow-sprinkles": "^0.17.1",
    "replace": "^1.2.2",
    "replace-in-file": "^6.3.5",
    "rimraf": "^5.0.0",
    "type-fest": "^4.2.0",
    "typescript": "^5.1.6",
    "vite": "^5.1.4",
    "vue": "^3.2.47",
    "vue-tsc": "^1.4.4"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.14.1",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/recipes": "^0.5.1",
    "animejs": "^3.2.1",
    "clsx": "^1.2.1",
    "immer": "^9.0.21",
    "vue": "^3.2.47",
    "zustand": "^4.4.7"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "changelog": {
    "labels": {
      "feat": "New Feature",
      "fix": "Bug Fix",
      "doc": "Documentation",
      "refactor": "Refactor",
      "breaking": "Breaking Change",
      "enhancement": "Enhancement",
      "chore": "Chore"
    }
  }
}
